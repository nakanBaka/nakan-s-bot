exports.run = (client) => {
  const config = require('./config.json');
  console.log("...");
}

client.on('message', message => {
    const swearWords = ["teste", "porra","caralho", "merda", "lixo", "gay", "puta", "puto"];
    if( swearWords.some(word => message.content.includes(word)) ) {
        message.delete();
        message.author.send('Mano, sem palavrÃ£o!');
      }
})
